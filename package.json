{
  "name": "weather-subscribe-service",
  "version": "1.0.0",
  "description": "weather service",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "production": "node -r  ts-node/register -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon --watch src --exec tsx src/index.ts",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "prepare": "husky install",
    "migrate": "ts-node -r tsconfig-paths/register node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "ts-node -r tsconfig-paths/register node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed": "ts-node -r tsconfig-paths/register node_modules/.bin/sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.5.0",
    "eslint-plugin-n": "^17.18.0",
    "express": "^5.1.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-cli-ts-js": "^1.1.2",
    "sequelize-typescript": "^2.1.6",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.2",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^8.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
